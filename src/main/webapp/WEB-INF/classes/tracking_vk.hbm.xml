            </key>
            <one-to-many class="com.mcm2.tracking.Hamlet"/>
        </set>
        <set name="bloodSlides" table="village" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="village"/>
            </key>
            <one-to-many class="com.mcm2.tracking.BloodSlide"/>
        </set>
